{
    "preset": "psr12",
    "rules": {
        "declare_strict_types": true,
        "fully_qualified_strict_types": true,
        "elseif": true,
        "ordered_imports": {
            "sort_algorithm": "length"
        },
        "blank_line_between_import_groups": false,
        "ordered_class_elements": true
    }
}
