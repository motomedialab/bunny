{
  "name": "motomedialab/bunny",
  "description": "An integration with BunnyCDN to upload and check on the status of streamed videos",
  "version": "1.0.0",
  "keywords": [
    "bunnycdn",
    "bunny-stream",
    "motomedialab"
  ],
  "homepage": "https://github.com/motomedialab/bunny",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Page",
      "email": "hello@motomedialab.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "laravel/framework": "^12.0",
    "motomedialab/connector": "^1.0"
  },
  "require-dev": {
    "larastan/larastan": "^v3.8.1",
    "laravel/pint": "^v1.23.0",
    "orchestra/testbench": "^v10.9.0",
    "pestphp/pest": "^v4.3.1"
  },
  "autoload": {
    "psr-4": {
      "Motomedialab\\Bunny\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Motomedialab\\Bunny\\": "src/",
      "Motomedialab\\Bunny\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motomedialab\\Bunny\\Providers\\BunnyServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}